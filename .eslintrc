{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/prefer-default-export": 0
  },
  "globals": {
    "test": true,
    "it": true,
    "expect": true,
    "window": true,
    "EventSource": true,
    "describe": true,
    "body": true,
    "browser": true,
    "jest": true
  }
}
